            data['gender_id'] = jQuery('#id_gender').val();
            data['age_id'] = jQuery('#id_age').val();
            data['social_class_id'] = jQuery('#id_social_class').val();

            // do ajax request and update html
            jQuery.get('/importer/api/b2b/', data, function(data) {

                jQuery('#js-div-sb-mediaplanningtool').html(data);
                jQuery('.js-sel-sb-mediaplanningtool').bind('change', filterDropdowns);
            });

        }

        jQuery('.js-sel-sb-mediaplanningtool').bind('change', filterDropdowns);

    });

