             */
            $form = jQuery('form#js-frm-sb-mediaplanningtool');

            values = $form.find(':input');
            values.each(function(){
                if (this.value != '' ){
                    $form.submit();
                }
            });

            return false;
        });

        var filterDropdowns = function() {

            // get values of all three dropdowns.
